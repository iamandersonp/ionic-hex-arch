{
  "name": "demo-app",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ionic": "npx ionic",
    "cordova": "npx cordova",
    "cypress": "npx cypress",
    "ng": "npx ng",
    "start": "npm run ionic serve",
    "build": "npx ionic build --prod",
    "test": "npx ng test --no-watch --detect-open-handles",
    "test:watch": "npx ng test --watch --detect-open-handles",
    "test:coverage": "npx ng test --no-watch --coverage  --detect-open-handles 2> /dev/null",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "res-generate": "npx cordova-res ios --skip-config --copy",
    "ios:rm": "rm -rf ios",
    "ios:add": "npx ionic capacitor add ios",
    "ios:build": "npx ionic cordova build --prod",
    "ios:build_TEST": "npx ionic build --prod",
    "ios:build_ANTE": "npx ionic build --prod",
    "ios:build_PROD": "npx cordova build --prod --device --release --buildConfig=build-ios_PROD.json",
    "android:rm": "rm -rf android",
    "android:add": "npx ionic capacitor add android",
    "android:build_TEST": "npx ionic build --prod",
    "android:build_ANTE": "npx ionic build --prod",
    "android:build_PROD": "npx cordova build --prod --device --release --buildConfig=build-ios_PROD.json",
    "build_TEST": "npx ionic build --configuration=scm",
    "build_ANTE": "npx ionic build --configuration=scm",
    "build_PROD": "npx ionic build --configuration=scm",
    "prettier:check": "prettier --check src/**/**/**/**/**/**/**/**/*.{html,ts,scss}",
    "prettier:write": "prettier --write src/**/**/**/**/**/**/**/**/*.{html,ts,scss}",
    "prettier:checkdc": "prettier --check documentation/**/**/**/**/**/**/**/**/*.js",
    "prettier:writedc": "prettier --write documentation/**/**/**/**/**/**/**/**/*.js",
    "setup:git-hooks": "git config core.hooksPath .git-hooks 2> /dev/null",
    "copy:enviroment": "cp src/environments/environment.prod{TEST}.ts src/environments/environment.prod.ts",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "postinstall": "npm run copy:enviroment && npm run setup:git-hooks"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@capacitor/android": "5.5.1",
    "@capacitor/app": "5.0.3",
    "@capacitor/core": "^5.5.1",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "5.5.1",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/status-bar": "5.0.4",
    "@ionic/angular": "^6.1.9",
    "@ionic/core": "^6.1.9",
    "@ionic/storage-angular": "^3.0.6",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "date-fns": "^2.28.0",
    "ionicons": "^6.0.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "16.0.1",
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular-eslint/builder": "~16.2.0",
    "@angular-eslint/eslint-plugin": "~16.2.0",
    "@angular-eslint/eslint-plugin-template": "~16.2.0",
    "@angular-eslint/template-parser": "~16.2.0",
    "@angular/cli": "^16.2.10",
    "@angular/compiler": "^16.2.12",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "^16.2.12",
    "@briebug/jest-schematic": "^6.0.0",
    "@capacitor/cli": "5.0.5",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@compodoc/compodoc": "^1.1.23",
    "@cypress/schematic": "^2.5.0",
    "@ionic/angular-toolkit": "^6.0.0",
    "@ionic/cli": "^6.18.0",
    "@types/jest": "^29.5.10",
    "@types/node": "~14.18.0",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "ajv-keywords": "^3.5.2",
    "cordova-res": "^0.15.4",
    "cypress": "13.4",
    "es6-promise-plugin": "^4.2.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jest": "29.7.0",
    "moment": "^2.29.4",
    "native-run": "^1.7.4",
    "prettier": "^2.8.8",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5"
  },
  "description": "An Ionic project"
}
